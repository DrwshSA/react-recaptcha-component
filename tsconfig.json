{
    "compilerOptions": {
      "target": "es5",
      "module": "es6",
      "lib": ["dom", "es2015", "es2016", "es2017", "es2018", "esnext"],
      "sourceMap": true,
      "jsx": "react",
      "outDir": "dist",
      "declaration": true,
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
    },
    "include": ["src"]
  }
  